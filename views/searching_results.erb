<div class="description">
  <h4>Searching tags: <em> <%= @tags_list %></em></h4>
  <br>
  <br>
  <h3>Attractions:</h3>
  <% if @attractions.any?() %>
    <ul>
      <% @attractions.each do |attraction| %>
      <li><a href="/attractions/<%= attraction.id() %>"><%= attraction.name() %></a></li>
      <% end %>
    </ul>
  <% else %>
    <p>There are no attractions matching this criteria! Try again!</p>
  <% end %>
</div>
<br>
<br>
<a href="/">Home page</a>
